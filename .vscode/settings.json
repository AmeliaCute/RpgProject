{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/**.asset": true,
        "**/**.cs.meta": true,
        "**/**.csproj": true,
        "**/**.meta": true,
        "Assets/Scripts": true,
        "Assets/HDRPDefaultResources": true,
        "Assets/Settings": true,
        "Library": true,
        "obj": true,
        "Packages": true,
        "ProjectSettings": true,
        "RpgProject.sln": true,
        ".vsconfig": true,
        ".vscode": true,
        "Logs": true,
        "UserSettings": true,
        "Temp": true,
        "**/*.prefab": true,
        ".vs": true,
        ".idea": true
    },
    "explorerExclude.backup": {},
    "hide-files.files": [
        ".vs",
        ".idea"
